<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="AnschreibenCreator.Lib\**" />
    <Compile Remove="CkWPF.MvvmBase\**" />
    <EmbeddedResource Remove="AnschreibenCreator.Lib\**" />
    <EmbeddedResource Remove="CkWPF.MvvmBase\**" />
    <None Remove="AnschreibenCreator.Lib\**" />
    <None Remove="CkWPF.MvvmBase\**" />
    <Page Remove="AnschreibenCreator.Lib\**" />
    <Page Remove="CkWPF.MvvmBase\**" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="AnschreibenCreator.Lib\AnschreibenCreator.Lib.csproj" />
    <ProjectReference Include="CkWPF.MvvmBase\CkWPF.Base.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Models\" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="AnschreibenCreator.Lib">
      <HintPath>AnschreibenCreator.Lib\bin\Debug\net9.0\AnschreibenCreator.Lib.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
