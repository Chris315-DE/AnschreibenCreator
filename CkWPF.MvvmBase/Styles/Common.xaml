<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="GUIForgroundDark">#212121</Color>
    <SolidColorBrush x:Key="GUIForgroundDarkBrush" Color="{StaticResource GUIForgroundDark}"/>
    <Color x:Key="GUIForgroundMid">#303030</Color>
    <SolidColorBrush x:Key="GUIForgroundMidBrush" Color="{StaticResource GUIForgroundMid}"/>
    <Color x:Key="GUIForgroundLight">#4a4a4a</Color>
    <SolidColorBrush x:Key="GUIForgroundLightBrush" Color="{StaticResource GUIForgroundLight}"/>



    <Color x:Key="DarkThemeBackground-a10">#121212</Color>
    <Color x:Key="DarkThemeBackground-a20">#282828</Color>
    <Color x:Key="DarkThemeBackground-a30">#3f3f3f</Color>
    <Color x:Key="DarkThemeBackground-a40">#575757</Color>
    <Color x:Key="DarkThemeBackground-a50">#717171</Color>
    <Color x:Key="DarkThemeBackground-a60">#8b8b8b</Color>

    <Color x:Key="DarKThemeGUIElement-a10">#8414ef</Color>
    <Color x:Key="DarKThemeGUIElement-a20">#963bf2</Color>
    <Color x:Key="DarKThemeGUIElement-a30">#a757f4</Color>
    <Color x:Key="DarKThemeGUIElement-a40">#b670f7</Color>
    <Color x:Key="DarKThemeGUIElement-a50">#c488f9</Color>
    <Color x:Key="DarKThemeGUIElement-a60">#d1a0fb</Color>




    <SolidColorBrush x:Key="DarkThemeBackgroundBrush-a10" Color="{StaticResource DarkThemeBackground-a10}"/>
    <SolidColorBrush x:Key="DarkThemeBackgroundBrush-a20" Color="{StaticResource DarkThemeBackground-a20}"/>
    <SolidColorBrush x:Key="DarkThemeBackgroundBrush-a30" Color="{StaticResource DarkThemeBackground-a30}"/>
    <SolidColorBrush x:Key="DarkThemeBackgroundBrush-a40" Color="{StaticResource DarkThemeBackground-a40}"/>
    <SolidColorBrush x:Key="DarkThemeBackgroundBrush-a50" Color="{StaticResource DarkThemeBackground-a50}"/>
    <SolidColorBrush x:Key="DarkThemeBackgroundBrush-a60" Color="{StaticResource DarkThemeBackground-a60}"/>

    <SolidColorBrush x:Key="DarKThemeGUIElementBrush-a10" Color="{StaticResource DarKThemeGUIElement-a10}"/>
    <SolidColorBrush x:Key="DarKThemeGUIElementBrush-a20" Color="{StaticResource DarKThemeGUIElement-a20}"/>
    <SolidColorBrush x:Key="DarKThemeGUIElementBrush-a30" Color="{StaticResource DarKThemeGUIElement-a30}"/>
    <SolidColorBrush x:Key="DarKThemeGUIElementBrush-a40" Color="{StaticResource DarKThemeGUIElement-a40}"/>
    <SolidColorBrush x:Key="DarKThemeGUIElementBrush-a50" Color="{StaticResource DarKThemeGUIElement-a50}"/>
    <SolidColorBrush x:Key="DarKThemeGUIElementBrush-a60" Color="{StaticResource DarKThemeGUIElement-a60}"/>




    <Color x:Key="ForegroundMain">#bed5ff</Color>
    <SolidColorBrush x:Key="ForegroundMainBrush" Color="{StaticResource ForegroundMain}"></SolidColorBrush>

    <Color x:Key="Background">#bfd9ff</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}"/>

    <Color x:Key="ForegroundLight">#FFFFFF</Color>
    <SolidColorBrush x:Key="ForegroundLightBrush" Color="{StaticResource ForegroundLight}"></SolidColorBrush>


    <Color x:Key="BackgroundLight">#efefef</Color>
    <SolidColorBrush x:Key="BrushBackroundLigth" Color="{StaticResource BackgroundLight}"/>

    <Color x:Key="WordBlue">#3099c5</Color>
    <SolidColorBrush x:Key="WordBlueBrush" Color="{StaticResource WordBlue}" />

    <Color x:Key="WordTransparentBlue">#003099c5</Color>
    <SolidColorBrush x:Key="WordTransparentBlueBrush" Color="{StaticResource WordTransparentBlue}" />

    <Color x:Key="WordLightBlue">#45b6e5</Color>
    <SolidColorBrush x:Key="WordLightBlueBrush" Color="{StaticResource WordLightBlue}" />

    <Color x:Key="WordVeryLightBlue">#c3e5f3</Color>
    <SolidColorBrush x:Key="WordVeryLightBlueBrush" Color="{StaticResource WordVeryLightBlue}" />

    <Color x:Key="WordVeryVeryLightBlue">#ecf6fb</Color>
    <SolidColorBrush x:Key="WordVeryVeryLightBlueBrush" Color="{StaticResource WordVeryVeryLightBlue}" />

    <Color x:Key="WordDarkBlue">#0c6991</Color>
    <SolidColorBrush x:Key="WordDarkBlueBrush" Color="{StaticResource WordDarkBlue}" />

    <Color x:Key="WordOrange">#ffa800</Color>
    <SolidColorBrush x:Key="WordOrangeBrush" Color="{StaticResource WordOrange}" />

    <Color x:Key="WordRed">#ff4747</Color>
    <SolidColorBrush x:Key="WordRedBrush" Color="{StaticResource WordRed}" />

    <Color x:Key="WordGreen">#00c541</Color>
    <SolidColorBrush x:Key="WordGreenBrush" Color="{StaticResource WordGreen}" />

    <FontFamily x:Key="LatoThin">pack://application;,,,/Fonts/#Lato Thin</FontFamily>
    <FontFamily x:Key="LatoRegular">pack://application;,,,/Fonts/#Lato Regular</FontFamily>
    <FontFamily x:Key="LatoHeavy">pack://application;,,,/Fonts/#Lato Heavy</FontFamily>
    <FontFamily x:Key="FA2">pack://application;,,,/Fonts/#FontAwesome</FontFamily>
    <FontFamily x:Key="FA">pack://application;,,,/Fonts/#Font Awesome 6 Free Solid</FontFamily>



    <Style TargetType="{x:Type Control}" x:Key="BaseStyle">
        <Setter Property="FontFamily" Value="{StaticResource LatoRegular}"/>
    </Style>

    <Style TargetType="{x:Type TextBlock}" x:Key="BaseTextblockStyle">
        <Setter Property="FontFamily" Value="{StaticResource FAnew}"/>
    </Style>



    <Style TargetType="{x:Type Button}" BasedOn="{StaticResource BaseStyle}" />
    <Style TargetType="{x:Type Label}" BasedOn="{StaticResource BaseStyle}" />
    <Style TargetType="{x:Type TextBox}" BasedOn="{StaticResource BaseStyle}" />
    <Style TargetType="{x:Type TextBlock}" BasedOn="{StaticResource BaseTextblockStyle}" />


</ResourceDictionary>